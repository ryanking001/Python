from sklearn.datasets import load_diabetes

# 당뇨병 데이터 세트 로드
diabetes = load_diabetes()

# 데이터와 타겟 확인
print("데이터 형태:", diabetes.data.shape)
print("타겟 형태:", diabetes.target.shape)
print("\n특성 이름:", diabetes.feature_names)
print("\n데이터 (처음 5개 샘플):\n", diabetes.data[:5])
print("\n타겟 (처음 5개 값):\n", diabetes.target[:5])

# 데이터셋에 대한 설명
print("\n데이터셋 설명:\n", diabetes.DESCR)
